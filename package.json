{
  "name": "kick-redirect",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun --hot index.ts",
    "build:css": "bunx @tailwindcss/cli -i ./styles.css -o ./public/styles.css --minify",
    "watch:css": "bunx @tailwindcss/cli -i ./styles.css -o ./public/styles.css --watch",
    "dev": "bun run build:css && bun --hot index.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.17"
  }
}